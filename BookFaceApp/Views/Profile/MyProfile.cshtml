@model IEnumerable<PublicationViewModel>

@{
	ViewBag.Title = "My Publications";
}

<h2 class="text-center">@User.Identity?.Name</h2>
<form asp-action="UploadProfilePicture" enctype="multipart/form-data" method="post" style="text-align: center;">
	<input id="file-upload" name="file" type="file" />
	<input type="submit" class="btn btn-primary" value="Upload" />
</form>
<h2 class="text-center">@ViewBag.Title</h2>
<hr />

<div class="row">
	@foreach (var publication in Model)
	{
		<partial name="_PublicationPartial" model="@publication" />
	}
</div>